{
    "$schema": "https://turbo.build/schema.json",
    "extends": ["//"],
    "pipeline": {
      "build": {
        "dependsOn": ["^build"],
        "outputs": [".next/**", "dist/**"]
      },
      "test": {
        "dependsOn": ["^build"],
        "outputs": []
      },
      "lint": {
        "dependsOn": ["build"],
        "outputs": []
      },
      "dev": {
        "cache": false,
        "dependsOn": ["lint"]
      },
      "start": {
        "cache": false
      },
      "clean": {
        "cache": false
      }
    }
  }
  